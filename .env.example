# Paper Trading Environment Variables Template
# Copy this file to .env and fill in your actual API keys

# Alpaca API Configuration (REQUIRED)
ALPACA_API_KEY=your_alpaca_api_key_here
ALPACA_SECRET_KEY=your_alpaca_secret_key_here
ALPACA_BASE_URL=https://paper-api.alpaca.markets

# LLM API Keys (OPTIONAL - for Multi-LLM Agent)
GROQ_API_KEY=your_groq_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
XAI_API_KEY=your_xai_api_key_here
DEEPSEEK_API_KEY=your_deepseek_api_key_here

# Trading Configuration (OPTIONAL - can be overridden via command line)
TRADING_SYMBOLS=SPY,QQQ
CHECK_INTERVAL=60
RISK_PER_TRADE=0.02
MAX_DRAWDOWN=0.05